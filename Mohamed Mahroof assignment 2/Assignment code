import random
while (True):
    temperature = random.randint(10,99)
    humidity = random.randint(10,99)
    print("Current temperature is ",temperature)
    print("Current humidity is ",humidity)
    temperature_ref = 37
    humidity_ref = 35
    if temperature > temperature_ref and humidity < humidity_ref:
        print("Alarm is on")
    else:
        print("Alarm is off")
        break